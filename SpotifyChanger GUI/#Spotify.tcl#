proc vTclWindow.top44 {base} {
    global vTcl
    if {$base == ""} {
        set base .top44
    }
    if {[winfo exists $base]} {
        wm deiconify $base; return
    }
    set top $base
    ###################
    # CREATING WIDGETS
    ###################
    vTcl::widgets::core::toplevel::createCmd $top -class Toplevel \
        -menu "$top.m50" -background #ffffff \
        -highlightbackground $vTcl(actual_gui_bg) -highlightcolor black 
    wm focusmodel $top passive
    wm geometry $top 400x200+814+321
    update
    # set in toplevel.wgt.
    global vTcl
    global img_list
    set vTcl(save,dflt,origin) 0
    wm maxsize $top 1924 1061
    wm minsize $top 120 1
    wm overrideredirect $top 0
    wm resizable $top 0 0
    wm deiconify $top
    wm title $top "New Toplevel"
    vTcl:DefineAlias "$top" "Toplevel1" vTcl:Toplevel:WidgetProc "" 1
    set vTcl(real_top) {}
    vTcl:withBusyCursor {
    label $top.lab45 \
        -activebackground #f0f0f0f0f0f0 -activeforeground #000000 \
        -background #ffffff -disabledforeground #a3a3a3 \
        -font {-family Verdana -size 22 -weight bold -slant roman -underline 0 -overstrike 0} \
        -foreground #0080ff -highlightbackground $vTcl(actual_gui_bg) \
        -highlightcolor black -text {Login Info} 
    vTcl:DefineAlias "$top.lab45" "Label1" vTcl:WidgetProc "Toplevel1" 1
    label $top.lab46 \
        -activebackground #f9f9f9 -activeforeground black -background #ffffff \
        -disabledforeground #a3a3a3 \
        -font {-family Candara -size 14 -weight bold -slant roman -underline 0 -overstrike 0} \
        -foreground $vTcl(actual_gui_fg) \
        -highlightbackground $vTcl(actual_gui_bg) -highlightcolor black \
        -text {User ID:} 
    vTcl:DefineAlias "$top.lab46" "Label2" vTcl:WidgetProc "Toplevel1" 1
    vTcl:copy_lock $top.lab46
    entry $top.ent47 \
        -background white -disabledforeground #a3a3a3 \
        -font {-family {Microsoft New Tai Lue} -size 14 -weight bold -slant roman -underline 0 -overstrike 0} \
        -foreground $vTcl(actual_gui_fg) \
        -highlightbackground $vTcl(actual_gui_bg) -highlightcolor #02b002 \
        -insertbackground #000000 -selectbackground blue \
        -selectforeground #ea0000 -validate focus -width 194 
    vTcl:DefineAlias "$top.ent47" "Entry1" vTcl:WidgetProc "Toplevel1" 1
    vTcl:copy_lock $top.ent47
    label $top.lab47 \
        -activebackground #f9f9f9 -activeforeground black -background #ffffff \
        -disabledforeground #a3a3a3 \
        -font {-family Candara -size 14 -weight bold -slant roman -underline 0 -overstrike 0} \
        -foreground $vTcl(actual_gui_fg) \
        -highlightbackground $vTcl(actual_gui_bg) -highlightcolor black \
        -text Password: 
    vTcl:DefineAlias "$top.lab47" "Label2_1" vTcl:WidgetProc "Toplevel1" 1
    entry $top.ent48 \
        -background white -disabledforeground #a3a3a3 \
        -font {-family {Microsoft New Tai Lue} -size 14 -weight bold -slant roman -underline 0 -overstrike 0} \
        -foreground $vTcl(actual_gui_fg) \
        -highlightbackground $vTcl(actual_gui_bg) -highlightcolor #02b002 \
        -insertbackground #000000 -selectbackground blue \
        -selectforeground #ea0000 -validate focus -width 194 
    vTcl:DefineAlias "$top.ent48" "Entry1_1" vTcl:WidgetProc "Toplevel1" 1
    vTcl:copy_lock $top.ent48
    menu $top.m50 \
        -activebackground $vTcl(actual_gui_menu_analog) \
        -activeforeground #000000 -background $vTcl(actual_gui_menu_bg) \
        -font TkMenuFont -foreground $vTcl(actual_gui_menu_fg) -tearoff 0 
    button $top.but45 \
        -activebackground #fff9f9 -activeforeground #000000 \
        -background #1ed760 -disabledforeground #a3a3a3 \
        -font {-family {Arial Black} -size 18 -weight bold -slant roman -underline 0 -overstrike 0} \
        -foreground #ffffff -highlightbackground $vTcl(actual_gui_bg) \
        -highlightcolor black -pady 0 -relief ridge -text START 
    vTcl:DefineAlias "$top.but45" "Button1" vTcl:WidgetProc "Toplevel1" 1
    button $top.but49 \
        -activebackground $vTcl(analog_color_m) -activeforeground #000000 \
        -background #ffffff -disabledforeground #a3a3a3 \
        -foreground $vTcl(actual_gui_fg) \
        -highlightbackground $vTcl(actual_gui_bg) -highlightcolor black \
        -image logo_png -pady 0 -relief flat -text Button 
    vTcl:DefineAlias "$top.but49" "Button2" vTcl:WidgetProc "Toplevel1" 1
    label $top.lab48 \
        -activebackground #f9f9f9 -activeforeground black -anchor n \
        -background #fdfdfd -borderwidth 0 -disabledforeground #a3a3a3 \
        -font {-family 돋움 -size 6 -weight normal -slant roman -underline 0 -overstrike 0} \
        -foreground #d90005 -highlightbackground $vTcl(actual_gui_bg) \
        -highlightcolor #dd0016 -text dddddddddddddddddddddddd 
    vTcl:DefineAlias "$top.lab48" "Label3" vTcl:WidgetProc "Toplevel1" 1
    ###################
    # SETTING GEOMETRY
    ###################
    place $top.lab45 \
        -in $top -x 0 -relx 0.05 -y 0 -width 0 -relwidth 0.46 -height 0 \
        -relheight 0.205 -anchor nw -bordermode ignore 
    place $top.lab46 \
        -in $top -x 0 -relx 0.075 -y 0 -rely 0.2 -width 0 -relwidth 0.185 \
        -height 0 -relheight 0.155 -anchor nw -bordermode ignore 
    place $top.ent47 \
        -in $top -x 0 -relx 0.088 -y 0 -rely 0.35 -width 194 -relwidth 0 \
        -height 27 -relheight 0 -anchor nw -bordermode ignore 
    place $top.lab47 \
        -in $top -x 0 -relx 0.088 -y 0 -rely 0.55 -width 0 -relwidth 0.21 \
        -height 0 -relheight 0.155 -anchor nw -bordermode ignore 
    place $top.ent48 \
        -in $top -x 0 -relx 0.088 -y 0 -rely 0.7 -width 194 -relwidth 0 \
        -height 27 -relheight 0 -anchor nw -bordermode ignore 
    place $top.but45 \
        -in $top -x 0 -relx 0.625 -y 0 -rely 0.7 -width 127 -relwidth 0 \
        -height 44 -relheight 0 -anchor nw -bordermode ignore 
    place $top.but49 \
        -in $top -x 0 -relx 0.65 -y 0 -rely 0.14 -width 107 -relwidth 0 \
        -height 104 -relheight 0 -anchor nw -bordermode ignore 
    place $top.lab48 \
        -in $top -x 40 -y 170 -width 174 -relwidth 0 -height 31 -relheight 0 \
        -anchor nw -bordermode ignore 
    } ;# end vTcl:withBusyCursor 

    vTcl:FireEvent $base <<Ready>>
}



set btop ""
if {$vTcl(borrow)} {
    set btop .bor[expr int([expr rand() * 100])]
    while {[lsearch $btop $vTcl(tops)] != -1} {
        set btop .bor[expr int([expr rand() * 100])]
    }
}
set vTcl(btop) $btop
Window show .
Window show .top44 $btop
if {$vTcl(borrow)} {
    $btop configure -background plum
}

